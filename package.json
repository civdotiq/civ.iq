{
  "name": "civic-intel-hub",
  "version": "0.1.0",
  "private": true,
  "author": "Mark Sandford",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "clean": "rm -rf .next node_modules",
    "clean:install": "npm run clean && npm install",
    "process-census": "tsx scripts/process-census-data.ts",
    "validate-mappings": "tsx scripts/validate-mappings.ts",
    "lookup-zip": "tsx scripts/lookup-zip.ts",
    "complete-phase": "tsx scripts/complete-phase.ts",
    "process-zip-districts": "tsx scripts/process-zip-districts.ts",
    "test-phase3-integration": "tsx scripts/test-phase3-integration.ts",
    "security:audit": "npm audit --audit-level=moderate",
    "security:full": "npm run security:audit",
    "preinstall": "npx audit-ci --moderate",
    "security:emergency": "npm audit fix --force",
    "prepare": "husky"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
