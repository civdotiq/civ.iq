    "@redis/client": "^5.5.6",
    "@sentry/nextjs": "^9.35.0",
    "@types/d3": "^7.4.3",
    "@types/js-yaml": "^4.0.9",
    "@types/leaflet": "^1.9.19",
    "@types/qrcode": "1.5.5",
    "@types/redis": "^4.0.11",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "csv-parse": "6.1.0",
    "d3": "^7.9.0",
    "fast-xml-parser": "^5.2.5",
    "fuse.js": "7.1.0",
    "html2canvas": "1.4.1",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.25.0",
    "js-yaml": "^4.1.0",
    "leaflet": "^1.9.4",
    "limiter": "3.0.0",
    "lucide-react": "^0.525.0",
    "next": "^15.3.5",
    "qrcode": "1.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.1",
    "react-window": "1.8.11",
    "react-window-infinite-loader": "1.0.10",
    "recharts": "^3.0.0",
    "redis": "^5.5.6",
    "swr": "2.3.4",
    "tailwind-merge": "^3.3.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/papaparse": "5.3.16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-window": "1.8.8",
    "@types/supertest": "^6.0.3",
    "audit-ci": "^7.1.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "17.2.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "glob": "^8.1.0",
    "husky": "9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "15.5.2",
    "msw": "^2.10.3",
    "npm-audit-resolver": "^3.0.0-RC.0",
    "papaparse": "5.5.3",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "5.3.14",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
