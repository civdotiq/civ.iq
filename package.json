{
  "name": "civic-intel-hub",
  "version": "0.1.0",
  "private": true,
  "author": "Mark Sandford",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "seed-data": "tsx scripts/seed-congress-data.ts",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "vitest watch",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "clean": "rm -rf .next node_modules",
    "clean:install": "npm run clean && npm install",
    "process-census": "tsx scripts/process-census-data.ts",
    "validate-mappings": "tsx scripts/validate-mappings.ts",
    "lookup-zip": "tsx scripts/lookup-zip.ts",
    "complete-phase": "tsx scripts/complete-phase.ts",
    "process-zip-districts": "tsx scripts/process-zip-districts.ts",
    "test-phase3-integration": "tsx scripts/test-phase3-integration.ts",
    "process-district-boundaries": "node scripts/process-district-boundaries.mjs",
    "process-real-census-districts": "node scripts/process-real-census-districts.mjs",
    "test-district-accuracy": "node scripts/test-district-accuracy.mjs",
    "migrate:119th": "npx tsx src/scripts/migrate-119th.ts",
    "validate:data": "npx tsx src/scripts/validate-data.ts",
    "validate-districts": "npx tsx scripts/validate-data.ts",
    "validate:all": "node scripts/validation/validate-all.js",
    "diagnose:apis": "tsx scripts/diagnose-apis.ts",
    "security:audit": "npm audit --audit-level=moderate",
    "security:full": "npm run security:audit",
    "preinstall": "echo 'Skipping security audit for deployment'",
    "security:emergency": "npm audit fix --force",
    "prepare": "husky",
    "flatten": "node flatten-codebase.js",
    "flatten:src": "node flatten-codebase.js -i src -o .bmad-core/flattened-src.xml",
    "debug:rep-api": "node debug-rep-api.js",
    "verify:rendering": "node verify-rendering.js",
    "emergency:diagnose": "node emergency-diagnose.js",
    "test:performance": "node scripts/performance-test.mjs",
    "test:performance:quick": "node scripts/quick-performance-baseline.mjs",
    "test:load": "node scripts/load-test.mjs",
    "test:redis": "tsx scripts/test-redis-performance.ts",
    "validate:production": "tsx scripts/run-production-validation.ts",
    "validate:production:critical": "tsx scripts/run-production-validation.ts -- --critical-only",
    "validate:production:parallel": "tsx scripts/run-production-validation.ts -- --parallel",
    "analyze": "npx @next/bundle-analyzer",
    "perf:benchmark": "PERF_LOGGING=true tsx scripts/performance-benchmark.ts",
    "perf:analyze": "ANALYZE=true npm run build",
    "perf:report": "tsx -e \"const {metricsCollector} = require('./src/lib/performance/metrics-collector'); console.log(metricsCollector.generateReport())\"",
    "perf:baseline": "npm run perf:benchmark && mv performance-results-*.json performance-baseline.json"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@mapbox/mbtiles": "0.12.1",
    "@mapbox/vector-tile": "2.0.4",
    "@redis/client": "^5.5.6",
    "@types/d3": "^7.4.3",
    "@types/js-yaml": "4.0.9",
    "@types/qrcode": "1.5.5",
    "autoprefixer": "^10.4.21",
    "cli-progress": "3.12.0",
    "clsx": "^2.1.1",
    "critters": "0.0.25",
    "csv-parse": "6.1.0",
    "d3": "^7.9.0",
    "fast-xml-parser": "5.2.5",
    "fuse.js": "7.1.0",
    "html2canvas": "1.4.1",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "2.16.0",
    "js-yaml": "4.1.0",
    "limiter": "3.0.0",
    "lru-cache": "10.4.3",
    "lucide-react": "^0.525.0",
    "maplibre-gl": "5.7.0",
    "next": "15.5.2",
    "pbf": "4.0.1",
    "pmtiles": "4.3.0",
    "qrcode": "1.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.5.2",
    "react-window": "1.8.11",
    "react-window-infinite-loader": "1.0.10",
    "recharts": "^3.0.0",
    "redis": "^5.5.6",
    "rss-parser": "3.13.0",
    "swr": "2.3.4",
    "tailwind-merge": "^3.3.1",
    "zod": "4.1.11",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cli-progress": "3.11.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/papaparse": "5.3.16",
    "@types/pbf": "3.0.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-window": "1.8.8",
    "@types/supertest": "^6.0.3",
    "audit-ci": "^7.1.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "17.2.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "glob": "^8.1.0",
    "husky": "9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "15.5.2",
    "mapshaper": "0.6.111",
    "msw": "^2.10.3",
    "npm-audit-resolver": "^3.0.0-RC.0",
    "papaparse": "5.5.3",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "5.3.14",
    "tsx": "4.20.5",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}
